  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/go-mini-kv-store",
      "env": {
        "PORT": "3000",
        "GIN_MODE": "debug"
      }
    },
    {
      "name": "Test e2e",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/test/e2e",
      "env": {
        "URL": "http://localhost:3000"
      },
      "showLog": true
    }
  ]
}
